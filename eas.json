{
  "cli": {
    "version": ">= 7.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "simulator": true
      },
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "release-candidate": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "releaseCandidate",
        "EXPO_PUBLIC_FEATURE_TEST_DATA_UI":"true"
      },
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      },
      "autoIncrement": true
    },
    "pre-release": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preRelease"
      },
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
