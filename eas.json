{
  "cli": {
    "version": ">= 7.8.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "base": {
      "android": {
        "image": "latest",
        "node": "20.17.0"
      },
      "ios": {
        "image": "latest",
        "node": "20.17.0"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "release-candidate": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "releaseCandidate"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      },
      "autoIncrement": true
    },
    "pre-release": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
